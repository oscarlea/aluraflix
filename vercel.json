{ 
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "http://localhost:4000/api/:path*"
      }
    ],

    "functions": {
      "src/api/server.js": {
        "memory": 1024,
        "maxDuration": 10
      },
      "src/api/*.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }

}

